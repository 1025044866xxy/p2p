<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xxy.p2p.dao.mapper.LoginHistoryDAO">

    <insert id="insert">
        insert into login_history(user_id, user_name, ip)
        values(#{userId}, #{userName}, #{ip})
    </insert>
</mapper>